{
  "root": "packages/ui-lib",
  "projectType": "library",
  "sourceRoot": "packages/ui-lib/src",
  "targets": {
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "npm run build",
            "forwardAllArgs": true
          }
        ],
        "cwd": "packages/ui-lib",
        "parallel": false
      }
    },
    "serve": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "npm run dev",
            "forwardAllArgs": true
          }
        ],
        "cwd": "packages/ui-lib",
        "parallel": false
      }
    },
    "lint": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "npm run lint",
        "cwd": "packages/ui-lib"
      }
    },
    "test": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "npm run test:unit",
        "cwd": "packages/ui-lib"
      }
    }
  },
  "tags": []
}
